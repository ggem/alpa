#!/bin/tcsh -f

sed -e 's/[():]//g'			\
    -e 's/merge/mrg/'			\
    -e 's/select/sel/'			\
    -e 's/insert/i/'			\
    -e 's/nreverse/nrevers/'		\
    -e 's/ milli.*//'			\
    -e 's/ \(..\)	/ 00\1	/'	\
    -e 's/ \(...\)	/ 0\1	/'	\
    -e 's/0000[0-9]*/00/'		\
    -e 's/3333[0-9]*/33/'		\
    -e 's/6666[0-9]*/67/'		\
    -e 's/09999[0-9]*/10/'		\
    -e 's/19999[0-9]*/20/'		\
    -e 's/29999[0-9]*/30/'		\
    -e 's/39999[0-9]*/40/'		\
    -e 's/49999[0-9]*/50/'		\
    -e 's/59999[0-9]*/60/'		\
    -e 's/69999[0-9]*/70/'		\
    -e 's/79999[0-9]*/80/'		\
    -e 's/89999[0-9]*/90/'		\
    -e 's/ /	/'			\
    -e 's/		/	/g'	\
  | sort
